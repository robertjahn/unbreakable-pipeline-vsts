{
  "id": "d34b9fb1-f1c7-4713-a0d6-e3f8d694f2f8",
  "name": "dynatraceunbreakablegate",
  "friendlyName": "1.1.25 Dynatrace Unbreakable Release Gate",
  "description": "Uses Dynatrace monitoring to determine if gate should pass or fail.",
  "author": "Abel Wang",
  "helpMarkDown": "",
  "category": "Utility",
  "visibility": [
    "Build",
    "Release"
  ],
  "runsOn": [
    "ServerGate"
  ],
  "demands": [],
  "version": {
    "Major": "1",
    "Minor": "1",
    "Patch": "25"
  },
  "instanceNameFormat": "Dynatrace Unbreakable Pipeline Release Gate $(message)",
  "inputs": [
    {
      "name": "unbreakableGateFunctionUrl",
      "type": "string",
      "label": "Unbreakable Gate Function Url",
      "defaultValue": "$(unbreakableGateFunctionUrl)",
      "required": true,
      "helpMarkDown": "URL to Azure Function hosting Dynatrace Unbreakable Gate"
    },
    {
      "name": "unbreakableGateFunctionKey",
      "type": "string",
      "label": "Unbreakable Gate Function Key",
      "defaultValue": "$(unbreakableGateFunctionKey)",
      "required": true,
      "helpMarkDown": "Secret Key to Azure Function hosting Dynatrace Unbreakable Gate"
    },
    {
      "name": "monspecUrl",
      "type": "string",
      "label": "Monspec Url",
      "defaultValue": "$(monspecUrl)",
      "required": true,
      "helpMarkDown": "Url to the Dyntrace Monspec file."
    },
    {
      "name": "pipelineInfoUrl",
      "type": "string",
      "label": "Pipeline Info Url",
      "defaultValue": "$(pipelineInfoUrl)",
      "required": true,
      "helpMarkDown": "Url to the pipeline info file."
    },
    {
      "name": "dynatraceTenantUrl",
      "type": "string",
      "label": "Dynatrace Tenant Url",
      "defaultValue": "$(dynatraceTenantUrl)",
      "required": true,
      "helpMarkDown": "Url to the Dynatrace Tenant."
    },
    {
      "name": "dynatraceApiToken",
      "type": "string",
      "label": "Dynatrace API Token",
      "defaultValue": "$(dynatraceApiToken)",
      "required": true,
      "helpMarkDown": "API token for your Dynatrace Tenant."
    },
    {
      "name": "dynatraceProxyUrl",
      "type": "string",
      "label": "Dynatrace Proxy Url",
      "defaultValue": "$(dynatraceProxyUrl)",
      "required": true,
      "helpMarkDown": "URL to the Dynatrace monspec proxy."
    },
    {
      "name": "serviceToCompare",
      "type": "string",
      "label": "Service To Compare or Pull",
      "defaultValue": "$(serviceToCompare)",
      "required": true,
      "helpMarkDown": "The Service To Compare for the Dynatrace Unbreakable Gate."
    },
    {
      "name": "compareType",
      "type": "pickList",
      "label": "MonSpec Compare Type",
      "defaultValue": "pullcompare",
      "required": true,
      "helpMarkDown": "Valid values are pullcompare or pull.",
      "options": {
        "pullcompare": "pullcompare",
        "pull": "pull"
      }
    },
    {
      "name": "compareWindow",
      "type": "string",
      "label": "Compare Window",
      "defaultValue": "$(compareWindow)",
      "required": true,
      "helpMarkDown": "The number of minutes to compare."
    },
    {
      "name": "compareShift",
      "type": "string",
      "label": "Compare Time Shift",
      "defaultValue": "$(compareShift)",
      "helpMarkDown": "Used for comparetype=pull only. The number of minutes to shift relative to now."
    },
    {
      "name": "PlanUrl",
      "type": "string",
      "label": "VSTS Plan Url",
      "defaultValue": "$(system.CollectionUri)",
      "required": true,
      "helpMarkDown": "Url the the VSTS collection plan (e.g. https://vsrm.dev.azure.com/robjahn/)."
    },
    {
      "name": "ProjectId",
      "type": "string",
      "label": "VSTS Project Id",
      "defaultValue": "$(system.TeamProjectId)",
      "required": true,
      "helpMarkDown": "Id to the VSTS Team Project."
    },
    {
      "name": "HubName",
      "type": "string",
      "label": "Hub Name",
      "defaultValue": "$(system.HostType)",
      "required": true,
      "helpMarkDown": "Where the task is being run. Either Hub or Server."
    },
    {
      "name": "PlanId",
      "type": "string",
      "label": "Plan Id",
      "defaultValue": "$(system.PlanId)",
      "required": true,
      "helpMarkDown": "VSTS Plan Id."
    },
    {
      "name": "JobId",
      "type": "string",
      "label": "Job Id",
      "defaultValue": "$(system.JobId)",
      "required": true,
      "helpMarkDown": "VSTS Job Id."
    },
    {
      "name": "TimelineId",
      "type": "string",
      "label": "Timeline Id",
      "defaultValue": "$(system.TimelineId)",
      "required": true,
      "helpMarkDown": "VSTS Timeline Id."
    },
    {
      "name": "TaskInstanceId",
      "type": "string",
      "label": "Task Instance Id",
      "defaultValue": "$(system.TaskInstanceId)",
      "required": true,
      "helpMarkDown": "Task Instance Id."
    },
    {
      "name": "AuthToken",
      "type": "string",
      "label": "Authentication Token",
      "defaultValue": "$(system.AccessToken)",
      "required": true,
      "helpMarkDown": "VSTS authentication token."
    }
  ],
  "execution": {
    "HttpRequest": {
      "Execute": {
        "endpointId": "",
        "endpointUrl": "$(unbreakableGateFunctionUrl)?code=$(unbreakableGateFunctionKey)",
        "method": "POST",
        "body": "{ \"Content-Type\":\"application/json\", \"PlanUrl\": \"$(PlanUrl)\", \"ProjectId\": \"$(ProjectId)\", \"HubName\": \"$(HubName)\", \"PlanId\": \"$(PlanId)\", \"JobId\": \"$(JobId)\", \"TimelineId\": \"$(TimelineId)\", \"TaskInstanceId\": \"$(TaskInstanceId)\", \"AuthToken\": \"$(AuthToken)\", \"monspecUrl\": \"$(monspecUrl)\", \"pipelineInfoUrl\": \"$(pipelineInfoUrl)\", \"dynatraceTennantUrl\": \"$(dynatraceTenantUrl)\", \"dynatraceToken\": \"$(dynatraceApiToken)\", \"proxyUrl\": \"$(dynatraceProxyUrl)\", \"serviceToCompare\":\"$(serviceToCompare)\", \"compareWindow\": \"$(compareWindow)\", \"compareShift\": \"$(compareShift)\", \"compareType\": \"$(compareType)\" }",
        "headers": "{ \"Content-Type\":\"application/json\"  }",
        "WaitForCompletion": true,
        "Expression": ""
      }
    }
  }
}